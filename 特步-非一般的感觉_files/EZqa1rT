var MycookStr = new String(document.cookie);
var MycookHead = "bancon_fa=";
if (MycookStr.indexOf(MycookHead) < 0) {
    var cooktime = new Date();
    cooktime.setDate(cooktime.getDate() + 7);
    document.cookie = "bancon_fa=yes;expires=" + cooktime.toGMTString();
    try {
        if (navigator.userAgent.match(/iPad|iPod|iPhone|Mobile|UP.Browser|Android|BlackBerry|Windows CE|Windows Phone|Nokia|webOS|SonyEricsson|Opera Mini|opera mobi|IEMobile|POLARIS/) != null) {
